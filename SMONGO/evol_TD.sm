ss 1
  #save parameter session
  if ($1 == 0) {
    echo WARNING : 0 reserved for system
    echo command aborted 
    return
  }
  if ($isave_$1 != 0) {
    echo session already saved
    define proceed ?
    if ('$proceed' !=  'y') {return}
  }
  define isave_$1 1
  savevar $1

rs 1
  #restore parameter session
  if ($1 == 0) {
    echo WARNING : 0 reserved for system
    echo command aborted 
    return
  }
  if ($isave_$1 == 0) {
    echo session does not exists
    return
  }
  restorevar $1

