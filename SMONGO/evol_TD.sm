    set yvarp = $1_$(nn[$j]) if ($1_$(nn[$j]) > $yinflog)
    set x1p = $x"_"$(nn[$j]) if ($1_$(nn[$j]) > $yinflog)
    set yvarn = $1_$(nn[$j]) if ($1_$(nn[$j]) < 0 && abs($1_$(nn[$j])) > $yinflog)
    set x1n = $x"_"$(nn[$j]) if ($1_$(nn[$j]) < 0 && abs($1_$(nn[$j])) > $yinflog)
    define n1 dimen(x1p)
    define n2 dimen(x1n)
    define nn ($n1+$n2)
    set xx = 1,$nn
    set yy = 1,$nn
    set zz = 1,$nn
    do i = 1,($n1-1) {
      set xx[$i] = x1p[$i]
      set yy[$i] = yvarp[$i]
      set zz[$i] = 1}
    do i = $n1,($n1+$n2-1) {
      define ii ($i-$n1)
      set xx[$i] = x1n[$ii]
      set yy[$i] = -yvarn[$ii] 
      set zz[$i] = -1}
    sort {xx yy zz}
    set yy = lg(yy)
    define kk ($k-1)
