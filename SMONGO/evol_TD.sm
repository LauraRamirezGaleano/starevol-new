    set var = yy
    set xvar = $xsmooth"_"$(nn[$j])
    if ($nxsmooth_$(nn[$j]) > 2) {smoothvar $nxsmooth_$(nn[$j]) $ibeg_$(nn[$j]) $iend_$(nn[$j])}
