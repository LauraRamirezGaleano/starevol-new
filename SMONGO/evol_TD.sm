    if ($xx == 1) { limits $xinf $xsup $yinfp $ysup }
    if ($xx == -1) { limits $xinf $xsup $yinfn $ysupn }
    relocate $(xx[1]) $(yy[1])
    do i = 1,($n1+$n2-1) {
      define i1 ($i+1)
      define xx $(zz[$i])
      if ($xx == 1) { limits $xinf $xsup $yinfp $ysup }
      if ($xx == -1) { limits $xinf $xsup $yinfn $ysupn }
      draw $(xx[$i]) $(var[$i]) }
  }
