  ptype 6 3 points (Teff_21[440]) (lg(L_21[440]))
  ptype 6 3 points (Teff_21[1005]) (lg(L_21[1005]))
  expand 2.8

  location 3500 31000 3500 31000
  w
  limits 0 1 0 1
  relocate 0.15 0.9 putlabel 6 1.5 M_\odot
  rv 3

myU
  #readsi n0000444 32
  #readsi n0001005 42
