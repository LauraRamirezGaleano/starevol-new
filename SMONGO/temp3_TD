  window 3 3 3 2
  nn 12
  iylabel 1
  init2dart 12
  define nbrcontour 5
  iso2dart xlambda 12
  limits 0 1 0 1
  relocate 0.5 1.1 putlabel 5 \delta \mu(r,\theta)
  horibar 24500 9500 5000 400

  psp
  
  iylabel 1
