  #ctype green   label N-G
  relocate $(($xsup-$xinf)*0.15) 1
  ctype red label  \Tau_{B1}\propto (1-\frac{\Omega^2}{2\pi G\rho}   )\Omega^2d_r(r^2/\rho)
  relocate $(($xsup-$xinf)*0.15) 2.5
  ctype cyan label  \Tau_{B2}\propto (a*(\psi\Lambda-\delta\Psi)-\frac{\epsilon-\epsilon_g}{\epsilon_m}) \Omega^2d_r(r^2/\rho)
  relocate $(($xsup-$xinf)*0.15) -0.5
  ctype green3 label  \Tau_{B3}\propto -\frac{\rho_m}{\rho}(\delta \Lambda_2-\delta \Psi_2)
  ctype black expand 1.5
  toplabel log(|S transport|)
  
