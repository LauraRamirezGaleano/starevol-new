**    1 S  34 ( 1 proto, 0 gamma)  1 Cl 35
      if (t9.lt.hfv(34,8)) then
         v( 167) = 1.0d-99
      else
         vhf = hffits (hfv(34,1),hfv(34,2),hfv(34,3),hfv(34,4),hfv(34,
     &        5),hfv(34,6),hfv(34,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 167) = vhf*rho
      endif
**    1 S  35 ( 0 betam, 0 nutri)  1 Cl 35
      v( 168) = 9.116519105d-8
**    1 S  35 ( 1 neutr, 0 gamma)  1 S  36
      if (t9.lt.hfv(35,8)) then
         v( 169) = 1.0d-99
      else
         vhf = hffits (hfv(35,1),hfv(35,2),hfv(35,3),hfv(35,4),hfv(35,
     &        5),hfv(35,6),hfv(35,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 169) = vhf*rho
      endif
**    1 S  35 ( 1 proto, 0 gamma)  1 Cl 36
      if (t9.lt.hfv(36,8)) then
         v( 170) = 1.0d-99
      else
         vhf = hffits (hfv(36,1),hfv(36,2),hfv(36,3),hfv(36,4),hfv(36,
     &        5),hfv(36,6),hfv(36,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 170) = vhf*rho
      endif
**    1 Cl 35 ( 1 neutr, 0 gamma)  1 Cl 36
      if (t9.lt.hfv(37,8)) then
         v( 171) = 1.0d-99
      else
         vhf = hffits (hfv(37,1),hfv(37,2),hfv(37,3),hfv(37,4),hfv(37,
     &        5),hfv(37,6),hfv(37,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 171) = vhf*rho
      endif
**    1 Cl 35 ( 1 proto, 0 gamma)  1 heavy
      if (t9.lt.hfv(38,8)) then
         v( 172) = 1.0d-99
      else
         vhf = hffits (hfv(38,1),hfv(38,2),hfv(38,3),hfv(38,4),hfv(38,
     &        5),hfv(38,6),hfv(38,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 172) = vhf*rho
      endif
**    1 S  36 ( 1 neutr, 0 gamma)  1 Cl 37
      if (t9.lt.hfv(39,8)) then
         v( 173) = 1.0d-99
      else
         vhf = hffits (hfv(39,1),hfv(39,2),hfv(39,3),hfv(39,4),hfv(39,
     &        5),hfv(39,6),hfv(39,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 173) = vhf*rho
      endif
**    1 S  36 ( 1 proto, 0 gamma)  1 Cl 37
      if (t9.lt.hfv(40,8)) then
         v( 174) = 1.0d-99
      else
         vhf = hffits (hfv(40,1),hfv(40,2),hfv(40,3),hfv(40,4),hfv(40,
     &        5),hfv(40,6),hfv(40,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 174) = vhf*rho
      endif
**    1 Cl 36 ( 0 betam, 0 nutri)  1 heavy
      v( 175) = 7.085325395d-14
**    1 Cl 36 ( 1 neutr, 0 gamma)  1 Cl 37
      if (t9.lt.hfv(41,8)) then
         v( 176) = 1.0d-99
      else
         vhf = hffits (hfv(41,1),hfv(41,2),hfv(41,3),hfv(41,4),hfv(41,
     &        5),hfv(41,6),hfv(41,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 176) = vhf*rho
      endif
**    1 Cl 36 ( 1 proto, 0 gamma)  1 Cl 37
      if (t9.lt.hfv(42,8)) then
         v( 177) = 1.0d-99
      else
         vhf = hffits (hfv(42,1),hfv(42,2),hfv(42,3),hfv(42,4),hfv(42,
     &        5),hfv(42,6),hfv(42,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 177) = vhf*rho
      endif
**    1 Cl 37 ( 1 neutr, 0 gamma)  1 heavy
      if (t9.lt.hfv(43,8)) then
         v( 178) = 1.0d-99
      else
         vhf = hffits (hfv(43,1),hfv(43,2),hfv(43,3),hfv(43,4),hfv(43,
     &        5),hfv(43,6),hfv(43,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 178) = vhf*rho
      endif
**    1 Cl 37 ( 1 proto, 0 gamma)  1 heavy
      if (t9.lt.hfv(44,8)) then
         v( 179) = 1.0d-99
      else
         vhf = hffits (hfv(44,1),hfv(44,2),hfv(44,3),hfv(44,4),hfv(44,
     &        5),hfv(44,6),hfv(44,7),t9inv,t913inv,t913,t9,t953,t9ln)
         v( 179) = vhf*rho
      endif
**    1 heavy ( 1 neutr, 0 gamma)  1 captn
      v( 180) = 8.1216d5
***   end of reaction rates

      return
      end



************************************************************************

       DOUBLE PRECISION FUNCTION EXV (x) 

************************************************************************
* Truncate the exponential function                                    *
* Input : exponential factor                                           *
************************************************************************

      implicit double precision (a-h,o-z)

      if (x.gt.150.d0) goto 10
      if (x.lt.-150.d0) goto 20
      goto 30
   10 exv = (x-149.d0)*exp(150.d0)
      return
   20 exv = -exp(-150.d0)/(x+149.d0)
      return
   30 exv = exp(x)

      return
      end



************************************************************************

      DOUBLE PRECISION FUNCTION HFFITS (a1,a2,a3,a4,a5,a6,a7,t91,t92,
     &     t93,t94,t95,t96)

************************************************************************
* Apply a fit to a nuclear reaction rate (from 'smoker')               *
* Input : temperature t9 and fit parameters ai                         *
************************************************************************

      implicit double precision (a-h,o-z)

      hffits = exv(a1+a2*t91+a3*t92+a4*t93+a5*t94+a6*t95+a7*t96)

      return
      end

