
      PROGRAM VITABLE

************************************************************************
*       tables for interpolation in T of nuclear reaction rates        *
************************************************************************

      implicit double precision (a-h,o-z)

      parameter(nre = 180)
      parameter(nreac = 44,npar = 8)

      dimension v(nre)

      common /hauserf/ hfv(nreac,npar)

      dimension tnuc9(250)

      open (unit = 10,file = 'vitsub.f')
      open (unit = 20,file = 'nuclfits.data')

      do i = 1,nreac
         read (20,1000)
         read (20,*) (hfv(i,j),j = 1,4)
         read (20,*) (hfv(i,j),j = 5,8)
      enddo

      rho = 1.d0
      t9l = -3.3142341d0
      del = 1.3204119d-2
      write (10,1000)
      write (10,1200)
      write (10,1000)
      write (10,1100)
      write (10,1300)
      write (10,1100)
      write (10,1000)
      write (10,1400)
      write (10,1500)

      do i = 1,250
         t9l = t9l+del
         tnuc9(i) = 10.d0**t9l
      enddo

      write (10,1501)
      do ii = 1,49
         iinf = 5*ii-4
         isup = iinf+4
         write (10,1502) (tnuc9(i),i = iinf,isup)
      enddo
      write (10,1503) (tnuc9(i),i = 246,250)
      write (10,1000)

      do i = 1,250
         t = tnuc9(i)*1.d9
         call vit (rho,tnuc9(i),v)
         do k = 1,nre
            if (v(k).lt.1.d-99) v(k) = 1.d-99
            v(k) = log10(v(k))
         enddo
         write (10,1600) t
         write (10,1700) i,i
         write (10,1800) (v(j),j = 1,7)
         write (10,1800) (v(j),j = 8,14)
         write (10,1800) (v(j),j = 15,21)
         write (10,1800) (v(j),j = 22,28)
         write (10,1800) (v(j),j = 29,35)
         write (10,1800) (v(j),j = 36,42)
         write (10,1800) (v(j),j = 43,49)
         write (10,1800) (v(j),j = 50,56)
         write (10,1800) (v(j),j = 57,63)
         write (10,1800) (v(j),j = 64,70)
         write (10,1800) (v(j),j = 71,77)
         write (10,1800) (v(j),j = 78,84)
         write (10,1800) (v(j),j = 85,91)
         write (10,1800) (v(j),j = 92,98)
         write (10,1800) (v(j),j = 99,105)
         write (10,1800) (v(j),j = 106,112)
         write (10,1800) (v(j),j = 113,119)
         write (10,1800) (v(j),j = 120,126)
         write (10,1800) (v(j),j = 127,133)
         write (10,1800) (v(j),j = 134,140)
         write (10,1800) (v(j),j = 141,147)
         write (10,1800) (v(j),j = 148,154)
         write (10,1800) (v(j),j = 155,161)
         write (10,1800) (v(j),j = 162,168)
         write (10,1800) (v(j),j = 169,175)
         write (10,1900) (v(j),j = 176,180)
      enddo
      write (10,1000)
      write (10,2000)
      write (10,1000)

      close (10)
      close (20)

 1000 format (1x)
 1100 format (72('*'))
 1200 format (6x,'BLOCK DATA VITSUB')
 1300 format ('*',11x,'tables of nuclear reaction rates in ',
     &     'function of T',10x,'*',/,'*',17x,'(see starevol.par for ',
     &     'the references)',16x,'*')
 1400 format (6x,'implicit none',//,
     &     6x,'double precision tnuc9,vrate',/,
     &     6x,'integer i,j',/)
 1500 format (6x,'common /nucvit/ tnuc9(250),vrate(250,180)')
 1501 format (1x,/,6x,'data (tnuc9(i),i = 1,250) /')
 1502 format (5x,'& ',5(1pd12.6,','))
 1503 format (5x,'& ',4(1pd12.6,','),1pd12.6,'/')
 1600 format (1x,/,'***** nav * <sig*v> / rho  at T = ',d10.4,' K')
 1700 format (6x,'data ((vrate(i,j),i = ',i3,',',i3,'),j = 1,180) /')
 1800 format (5x,'& ',7(f8.4,','))
 1900 format (5x,'& ',4(f8.4,','),f8.4,'/')
 2000 format (6x,'end',/)

      end



************************************************************************
 
      SUBROUTINE VIT (rho,t9,v)
 
************************************************************************
* nuclear reaction rates [rho*navogadro*(sigma*vit)maxwell] (1/sec)    *
************************************************************************
 
      implicit double precision (a-h,o-z)
 
      double precision na23pa,na23pg
 
      parameter (nre = 180)
      parameter (nreac = 44,npar = 8)
 
      common /hauserf/ hfv(nreac,npar)
 
      dimension v(nre)
 
      avn = 6.022169d23
      boltz = 1.380623d-16
      bboltz = boltz/1.60184d-09

      t9inv = 1.d0/t9
      t913 = t9**(1.d0/3.d0)
      t913inv = 1.d0/t913
      t914 = t9**(1.d0/4.d0)
      t912 = t914**2
      t915 = t9**(1.d0/5.d0)
      t923 = t913**2
      t927 = t9**(2.d0/7.d0)
      t932 = t9*t912
      t934 = t9**(3.d0/4.d0)
      t935 = t9**(3.d0/5.d0)
      t938 = t9**(3.d0/8.d0)
      t943 = t9*t913
      t953 = t9*t923
      t954 = t9*t914
      t974 = t9*t912*t914
      t965 = t935**2
      t9ln = log(t9)
      t6 = t9*1.d3
      t = t9*1.d9
 
      sn = 1.4428d5
 
***   screening put to unity
      ff = 1.d0
 
**    2 proto ( 0 gamma, 0 nutri)  1 deutr
c     z1 = 1.d0
c     z2 = 1.d0
c     aa = 0.5d0
c     s0 = 4.07d-22
c     ds0 = 4.52d-24
c     ttau = 42.487*(z1*z1*z2*z2*aa/t6)**(1/3.d0)
c     seff = s0*(1+5.d0/(12*ttau))+bboltz*t*ds0*(ttau+35.d0/12.d0)/3.d0
c     sigv = avn*1.3005d-15*(z1*z2/(aa*t6*t6))**(1/3.d0)*seff*exp(-ttau)
c     v(   1) = sigv*rho*ff
      v(   1) = (4.01d-15/t923*exv(-3.38/t913)*(1.+0.123*t913+1.09*t923+
     & 0.938*t9))*rho*ff
**    1 deutr ( 1 proto, 0 gamma)  1 He  3
c     z1 = 1.d0
c     z2 = 1.d0
c     aa = 2.d0/3.d0
c     s0 = 2.5d-4
c     ds0 = 7.9d-6
c     ttau = 42.487*(z1*z1*z2*z2*aa/t6)**(1/3.d0)
c     seff = s0*(1+5.d0/(12*ttau))+bboltz*t*ds0*(ttau+35.d0/12.d0)/3.d0
c     sigv = avn*1.3005d-15*(z1*z2/(aa*t6*t6))**(1/3.d0)*seff*exp(-ttau)
c     v(   2) = sigv*rho*ff
      v(   2) = (2.24d3/t923*exv(-3.720/t913)*(1.+0.112*t913+3.38*t923+
     & 2.65*t9))*rho*ff
**    2 deutr ( 0 gamma, 0 gamma)  1 alpha
      v(   3) = (4.84d1/t923*exv(-4.258/t913)*(1.+0.098*t913-0.203*t923-
     & 0.139*t9+0.106*t943+0.185*t953))*ff*rho
**    2 deutr ( 0 gamma, 1 neutr)  1 He  3
      v(   4) = (3.88d8/t923*exv(-4.258/t913)*(1.+0.098*t913+0.418*t923+
     & 0.287*t9+0.638*t943+1.112*t953))*ff*rho
**    1 He  3 ( 1 deutr, 1 proto)  1 alpha
      v(   5) = (5.86d10/t923*exv(-7.181/t913-(t9/0.315)**2)*(1.+0.058*
     & t913+0.142*t923+5.78d-2*t9+2.25*t943+2.32*t953)+
     & 4.36d8/t912*exv(-1.720/t9))*rho*ff
**    2 He  3 ( 0 gamma, 2 proto)  1 alpha
c     z1 = 2.d0
c     z2 = 2.d0
c     aa = 9.d0/6.d0
c     s0 = 5.15d3
c     ds0 = -0.9d0
c     ttau = 42.487*(z1*z1*z2*z2*aa/t6)**(1/3.d0)
c     seff = s0*(1+5.d0/(12*ttau))+bboltz*t*ds0*(ttau+35.d0/12.d0)/3.d0
c     sigv = avn*1.3005d-15*(z1*z2/(aa*t6*t6))**(1/3.d0)*seff*exp(-ttau)
c     v(   6) = sigv*rho*ff
c*    adding a resonance term
c     v6 = 6.04d+10/t923*exv(-12.276/t913)*(1.+0.034*t913-0.522*
c    & t923-0.124*t9+0.353*t943+0.213*t953)
c     gamwidth = 5.0d-22
c     eres = 3.0d0
c     gamwidth = 1.2d-19
c     eres = 1.0d1
c     vres = 8.29d13*gamwidth*t7**(-1.5d0)*exp(-1.1603*eres/t7)
c     v(   6) = (v6+vres)*rho*ff
      v(   6) = (6.04d+10/t923*exv(-12.276/t913)*(1.+0.034*t913-0.522*
     & t923-0.124*t9+0.353*t943+0.213*t953))*rho*ff
**    1 alpha ( 1 deutr, 0 gamma)  1 Li  6
      v(   7) = (3.01d1/t923*exv(-7.423/t913)*(1.+0.056*t913-4.85*t923+
     & 8.85*t9-0.585*t943-0.584*t953)+8.55d1/t932*exv(-8.228/t9))*rho
     & *ff
**    1 alpha ( 1 He  3, 0 gamma)  1 Be  7
c     z1 = 2.d0
c     z2 = 2.d0
c     aa = 12.d0/7.d0
c     s0 = 0.54d0
c     ds0 = -3.1d-4
c     ttau = 42.487*(z1*z1*z2*z2*aa/t6)**(1/3.d0)
c     seff = s0*(1+5.d0/(12*ttau))+bboltz*t*ds0*(ttau+35.d0/12.d0)/3.d0
c     sigv = avn*1.3005d-15*(z1*z2/(aa*t6*t6))**(1/3.d0)*seff*exp(-ttau)
c     v(   8) = sigv*rho*ff
      tt = t9/(1.+4.95d-02*t9)
      t56 = tt**(+5.d0/6.d0)
      t13 = tt**(+1.d0/3.d0)
      v(   8) = (5.61d+06*t56/t932*exv(-12.826/t13))*rho*ff
**    3 alpha ( 0 gamma, 0 gamma)  1 C  12
      f14 = 0.1
      if (t9.gt.0.08d0) then
      vsec = (2.79d-08/t9**3*exv(-4.4027/t9)+f14*1.35d-07/t932*exv
     & (-24.811/t9))*rho**2*ff
      else
      v1 = 7.4d+05/t932*exv(-1.0663/t9)+4.164d+09/t923*exv(-13.490/t913
     & -(t9/0.098)**2)*(1.+0.031*t913+8.009*t923+1.732*t9+49.883*
     & t943+27.426*t953)
      v2 = 1.30d+02/t932*exv(-3.3364/t9)+2.510d+07/t923*exv(-23.570/t913
     & -(t9/0.235)**2)*(1.+0.018*t913+5.249*t923+0.650*t9+19.176*t943
     & +6.034*t953)
      vsec = (2.9d-16*v1*v2*(0.01+0.2*(1.+4.*exv(-(0.025/t9)**3.263))
     & /(1.+4.*exv(-(t9/0.025)**9.227)))+f14*1.35d-07/t932*exv(-24.811
     & /t9))*ff*(rho**2)
      end if
      v(   9) = vsec
**    1 Li  6 ( 1 proto, 0 gamma)  1 Be  7
      t9bb = t9/(1.-9.69d-2*t9+2.84d-2*t953/(1.-9.69d-2*
     & t9)**(2.d0/3.d0))
      t9bb56 = t9bb**(5.d0/6.d0)
      t9bb13 = t9bb**(1.d0/3.d0)
      v(  10) = (6.69d5*t9bb56/t932*exv(-8.413/t9bb13))*rho*ff
**    1 Li  6 ( 1 proto, 1 He  3)  1 alpha
      v(  11) = (3.73d10/t923*exv(-8.413/t913-(t9/5.50)**2)*(1.+0.050*
     & t913-0.061*t923-0.021*t9+0.006*t943+0.005*t953)+1.33d10/t932*
     & exv(-17.763/t9)+1.29d9/t9*exv(-21.820/t9))*rho*ff
**    1 Li  7 ( 1 proto, 1 alpha)  1 alpha
c     z1 = 3.d0
c     z2 = 1.d0
c     aa = 7.d0/8.d0
c     s0 = 52.d0
c     ds0 = 0.d0
c     ttau = 42.487*(z1*z1*z2*z2*aa/t6)**(1/3.d0)
c     seff = s0*(1+5.d0/(12*ttau))+bboltz*t*ds0*(ttau+35.d0/12.d0)/3.d0
c     sigv = avn*1.3005d-15*(z1*z2/(aa*t6*t6))**(1/3.d0)*seff*exp(-ttau)
c     v(  12) = sigv*rho*ff
      t9a = t9/(1.d0+0.759*t9)
      t9a56 = t9a**(5.d0/6.d0)
      t9a13 = t9a**(1.d0/3.d0)
      v(  12) = rho*ff*(1.096d+09/t923*exv(-8.472/t913)-4.830d8*t9a56/
     & t932*exv(-8.472/t9a13)+1.06d+10/t932*exv(-30.442/t9)+1.56d5/
     & t923*exv(-8.472/t913-(t9/1.696)**2)*(1.+0.049*t913+2.498*t923+
     & 0.860*t9+3.518*t943+3.080*t953)+1.55d6/t932*exv(-4.478/t9))
**    1 Li  7 ( 1 deutr, 1 neutr)  2 alpha
      v(  13) = 2.92d11/t923*exv(-10.259/t913)*rho*ff
**    1 Be  7 ( 0 betap, 0 nutri)  1 Li  7
      t6 = t9*1.d3
      t62 = sqrt(t6)
      vsec = 5.6d-9/t62*(1.+0.004*(t6-16.d0))*rho
c     vsec = (1.34d-10/t912*(1.-0.537*t913+3.86*t923+0.0027/t9*
c    &     exv(2.515d-03/t9)))*rho
      if (t9.gt.1.d-3) goto 5
      vmax = 1.5032d-7
      vsec = min(vsec,vmax)
   5  v(  14) = vsec
**    1 Be  7 ( 1 proto, 0 gamma)  1 B   8
c     z1 = 4.d0
c     z2 = 1.d0
c     aa = 7.d0/8.d0
c     s0 = 0.0243
c     ds0 = -3.d-5
c     ttau = 42.487*(z1*z1*z2*z2*aa/t6)**(1/3.d0)
c     seff = s0*(1+5.d0/(12*ttau))+bboltz*t*ds0*(ttau+35.d0/12.d0)/3.d0
c     sigv = avn*1.3005d-15*(z1*z2/(aa*t6*t6))**(1/3.d0)*seff*exp(-ttau)
c     v(  15) = sigv*rho*ff
      v(  15) = (3.11d+05/t923*exv(-10.262/t913)+2.53d+03/t932*
     & exv(-7.306/t9))*rho*ff
**    1 Be  7 ( 1 deutr, 1 proto)  2 alpha
      v(  16) = (1.07d12/t923*exv(-12.428/t913))*rho*ff
**    1 B   8 ( 0 betap, 0 nutri)  2 alpha
      v(  17) = 8.96d-01
**    1 Be  9 ( 1 proto, 0 gamma)  1 B  10
      v(  18) = (1.33d7/t923*exv(-10.359/t913-(t9/0.846)**2)*(1.+0.040*
     & t913+1.52*t923+0.428*t9+2.15*t943+1.54*t953)+9.64d4/t932*exv(-
     & 3.445/t9)+2.72d6/t932*exv(-10.620/t9))*rho*ff
**    1 Be  9 ( 1 proto, 1 deutr)  2 alpha
      v(  19) = (2.11d11/t923*exv(-10.359/t913-(t9/0.520)**2)*(1.+0.040*
     & t913+1.09*t923+0.307*t9+3.21*t943+2.30*t953)+5.79d8/t9*exv(-
     & 3.046/t9)+8.50d8/t934*exv(-5.800/t9))*rho*ff
**    1 Be  9 ( 1 proto, 1 alpha)  1 Li  6
      v(  20) = (2.11d11/t923*exv(-10.359/t913-(t9/0.520)**2)*(1.+0.040*
     & t913+1.09*t923+0.307*t9+3.21*t943+2.30*t953)+4.51d8/t9*exv(-
     & 3.046/t9)+6.70d8/t934*exv(-5.160/t9))*rho*ff
**    1 B  10 ( 1 proto, 0 gamma)  1 C  11
      v(  21) = (4.61d5/t923*exv(-12.062/t913-(t9/4.402)**2)*(1.+0.035*
     & t913+0.426*t923+0.103*t9+0.281*t943+0.173*t953)+1.93d5/t932*
     & exv(-12.041/t9)+1.14d4/t932*exv(-16.164/t9))*rho*ff
**    1 B  10 ( 1 proto, 1 alpha)  1 Be  7
      v(  22) = (1.26d11/t923*exv(-12.062/t913-(t9/4.402)**2)*(1.+0.035*
     & t913-0.498*t923-0.121*t9+0.300*t943+0.184*t953)+2.59d9/t9*exv(-
     & 12.260/t9))*rho*ff
**    1 B  11 ( 1 proto, 0 gamma)  1 C  12
      v(  23) = ff*(4.62d+07/t923*exv(-12.095/t913-(t9/0.239)**2)
     & *(1.+0.035*t913+3.00*t923+0.723*t9+9.91*t943+6.07*t953)
     & +7.89d+03/t932*exv(-1.733/t9)+9.68d+04/t915*exv(-5.617/t9))*rho
**    1 B  11 ( 1 proto, 1 alpha)  2 alpha
      v(  24) = (2.20d+12/t923*exv(-12.095/t913-(t9/1.644)**2)
     & *(1.+0.034*t913+0.140*t923+0.034*t9+0.190*t943+0.116*t953)
     & +4.03d+06/t932*exv(-1.734/t9)+6.73d+09/t932*exv(-6.262/t9)
     & +3.88d+09/t9*exv(-14.154/t9))*rho*ff
**    1 B  11 ( 1 alpha, 1 proto)  1 C  14
      b11ap = (5.37d11/t923*exv(-28.234/t913-(t9/0.347)**2)*(1.+0.015*
     & t913+5.575*t923+0.576*t9+15.888*t943+4.174*t953)+5.44d-3/t932*
     & exv(-2.827/t9)+3.36d2/t932*exv(-5.178/t9)+5.32d6/t938*
     & exv(-11.617/t9))
      v(  25) = b11ap*rho*ff
**    1 C  11 ( 0 betap, 0 nutri)  1 B  11
      v(  26) = 5.64d-04
**    1 C  11 ( 1 proto, 0 gamma)  1 C  12
c     v(  27) = (4.24d+04/t923*exv(-13.658/t913-(t9/1.627)**2)
c    & *(1.+0.031*t913+3.11*t923+0.665*t9+4.61*t943+2.50*t953)
c    & +8.84d+03/t932*exv(-7.021/t9))*rho*ff
      v(  27) = (1.69d7*exv(-13.66/t913)/t923*(1.-7.868*t913+30.209*
     & t923-57.433*t9+53.492*t943-19.027*t953)+63.6/t932*exv(-4.178/
     & t9)+7.70d3/t932*exv(-6.847/t9))*rho*ff
**    1 C  12 ( 1 neutr, 0 gamma)  1 C  13
c     v(  28) = 1.68d-02*sn*rho
      v(  28) = (466.+5320.*t9)*rho
**    1 C  12 ( 1 proto, 0 gamma)  1 N  13
      v(  29) = (2.04d+07/t923*exv(-13.690/t913-(t9/1.5)**2)*(1.+0.030*
     & t913+1.19*t923+0.254*t9+2.06*t943+1.12*t953)+1.08d+05/t932
     & *exv(-4.925/t9)+2.15d+05/t932*exv(-18.179/t9))*rho*ff
**    1 C  12 ( 1 alpha, 0 gamma)  1 O  16
c     v(  30) = (1.04d+08/t9**2./(1.+0.0489/t923)**2.*exv(-32.12/t913-
c    & (t9/3.496)**2.)+1.76d+08/t9**2./(1.+0.2654/t923)**2.*exv(-32.12
c    & /t913)+1.25d+03/t932*exv(-27.499/t9)+1.43d-02*t9**5.*exv(-15.541
c    & /t9))*rho*ff
      v(  30) = (2.93d+08/t9**2./(1.+0.0489/t923)**2.*exv(-32.12/t913-
     & (t9/3.496)**2.)+3.14d+08/t9**2./(1.+0.2654/t923)**2.*exv(-32.12
     & /t913)+1.25d+03/t932*exv(-27.499/t9)+1.43d-02*t9**5.*exv(-15.541
     & /t9))*rho*ff
**    2 C  12 ( 0 gamma, 1 alpha)  1 Ne 20
      t9a = t9/(1.+0.0396*t9)
      t9a13 = t9a**(1.d0/3.d0)
      t9a56 = t9a**(5.d0/6.d0)
      vc12c12 = 4.27d26*t9a56/t932*exv(-84.165/t9a13-2.12d-3*t9**3)
      v(  31) = vc12c12*0.56d0*rho*ff
**    2 C  12 ( 0 gamma, 1 proto)  1 Na 23
      v(  32) = vc12c12*0.44d0*rho*ff
**    1 C  13 ( 1 neutr, 0 gamma)  1 C  14
      v(  33) = (182.+4.633d4/t932*exv(-1.636/t9))*rho
